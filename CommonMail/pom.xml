		</dependency>
		<dependency>
			<groupId>net.sourceforge.juniversalchardet</groupId>
			<artifactId>juniversalchardet</artifactId>
			<version>1.0.3.bundle</version>
		</dependency>
		<dependency>
			<groupId>it.eng.auriga</groupId>
			<artifactId>AurigaBusinessClient</artifactId>
			<version>1.0.1</version>
			<exclusions>
				<exclusion>
					<artifactId>log4j</artifactId>
					<groupId>log4j</groupId>
				</exclusion>
				<exclusion>
					<artifactId>it.eng.utility</artifactId>
					<groupId>core-module</groupId>
				</exclusion>
